  "type": "symfony-bundle",
  "authors": [
    {
      "name": "share",
      "email": "1337.um@gmail.com"
    }
  ],
  "keywords": [
    "admin bundle",
    "graph",
    "neo4j",
    "neo4j-php-ogm",
